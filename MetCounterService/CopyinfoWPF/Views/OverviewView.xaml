<Window
    x:Class="CopyinfoWPF.Views.OverviewView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:UControls="clr-namespace:CopyinfoWPF.UControls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:CopyinfoWPF.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="OverviewView"
    Width="400"
    Height="850"
    mc:Ignorable="d">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <UControls:UcClientOverview x:Name="ClientControl" />
            <UControls:UcDeviceOverview x:Name="DeviceControl" />
        </StackPanel>
    </Grid>
</Window>
